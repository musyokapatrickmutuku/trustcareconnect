{
  "dashboard": {
    "id": null,
    "title": "TrustCareConnect Overview",
    "tags": ["trustcareconnect", "overview"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "custom",
          "options": [
            {
              "text": "Production",
              "value": "production",
              "selected": true
            },
            {
              "text": "Staging",
              "value": "staging"
            },
            {
              "text": "Development",
              "value": "development"
            }
          ]
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Status Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=~\"ai-proxy|frontend|icp-backend\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP", 
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate (req/min)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"ai-proxy\"}[5m]) * 60",
            "legendFormat": "AI Proxy",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{job=\"frontend\"}[5m]) * 60",
            "legendFormat": "Frontend",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "unit": "reqps"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true
        }
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "graph", 
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"ai-proxy\"}[5m]))",
            "legendFormat": "AI Proxy",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"frontend\"}[5m]))",
            "legendFormat": "Frontend", 
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "unit": "s"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [2],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Response Time",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "title": "Error Rate (%)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "(rate(http_requests_total{job=\"ai-proxy\",status=~\"5..\"}[5m]) / rate(http_requests_total{job=\"ai-proxy\"}[5m])) * 100",
            "legendFormat": "AI Proxy 5xx",
            "refId": "A"
          },
          {
            "expr": "(rate(http_requests_total{job=\"ai-proxy\",status=~\"4..\"}[5m]) / rate(http_requests_total{job=\"ai-proxy\"}[5m])) * 100",
            "legendFormat": "AI Proxy 4xx",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ],
        "thresholds": [
          {
            "value": 5,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 5,
        "title": "AI Query Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(ai_queries_total{provider=\"openai\"}[5m])",
            "legendFormat": "OpenAI Queries",
            "refId": "A"
          },
          {
            "expr": "rate(ai_queries_total{provider=\"claude\"}[5m])",
            "legendFormat": "Claude Queries", 
            "refId": "B"
          },
          {
            "expr": "rate(ai_query_errors_total[5m])",
            "legendFormat": "Query Errors",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "unit": "reqps"
          }
        ]
      },
      {
        "id": 6,
        "title": "Log Volume by Level",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(loki_distributor_lines_received_total[5m])) by (level)",
            "legendFormat": "{{level}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "unit": "logs/s"
          }
        ],
        "stack": true
      },
      {
        "id": 7,
        "title": "Recent Errors",
        "type": "logs",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "{level=\"error\"}",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "sortOrder": "Descending",
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none"
        }
      },
      {
        "id": 8,
        "title": "System Resource Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          },
          {
            "expr": "(1 - (node_filesystem_avail_bytes{fstype!=\"tmpfs\"} / node_filesystem_size_bytes{fstype!=\"tmpfs\"})) * 100",
            "legendFormat": "Disk Usage %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ],
        "thresholds": [
          {
            "value": 80,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "-- Grafana --",
          "type": "dashboard",
          "iconColor": "rgba(0, 211, 255, 1)",
          "enable": true,
          "hide": false
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 1,
    "links": [],
    "liveNow": false,
    "style": "dark"
  }
}