{
  "version": 1,
  "canisters": {
    "backend": {
      "main": "packages/backend/src/main.mo",
      "type": "motoko"
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "frontend": {
        "entrypoint": "frontend-simple/index.html"
      },
      "source": [
        "frontend-simple/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    },
    "replica": {
      "subnet_type": "application"
    }
  },
  "output_env_file": ".env",
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "application"
      }
    }
  }
}